pipeline {
    agent any

    tools {
        maven 'maven'
    }

    stages {
        stage('gitcheckout'){
            steps{
                git branch: 'main', url: 'https://github.com/Luckyocloo/web-app.git'
            }
        }

        stage('maven build'){
            steps{
                sh 'mvn clean package'
            }
        }
    }
}    
    
